{
  "name": "graphzep-mcp-server",
  "version": "0.0.3",
  "description": "GraphZep MCP Server - Exposes GraphZep functionality through the Model Context Protocol",
  "main": "dist/graphzep-mcp-server.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/graphzep-mcp-server.js",
    "start:stdio": "node dist/graphzep-mcp-server.js --transport stdio",
    "start:sse": "node dist/graphzep-mcp-server.js --transport sse",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "commander": "^12.0.0",
    "dotenv": "^16.4.5",
    "graphzep": "^0.0.4",
    "uuid": "^10.0.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/uuid": "^10.0.0",
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
